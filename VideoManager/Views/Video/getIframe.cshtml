@model VideoManager.Models.Data.Video
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Midwest Streams</title>

    <link href="~/Content/bootstrap-viewing.css" rel="stylesheet" />
