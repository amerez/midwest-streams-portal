<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
  <add key="logFilePath" value="C:\BatchFiles\" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  <add key="ffmpeg" value="C:\ffmpeg\bin\ffmpeg.exe" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  <add key="rawArchive" value="C:\VideoArchive\RawVideoFiles" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  <add key="failArchive" value="C:\VideoArchive\FailedVideoFiles" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  <add key="convertedArchive" value="C:\VideoArchive\ConvertedVideos" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  <add key="tempEditFolder" value="C:\VideoArchive\TempEditFolder" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  <add key="IsAzureVM" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="tempEditFolder" value="C:\VideoArchive\Thumbnails" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
</configuration>

